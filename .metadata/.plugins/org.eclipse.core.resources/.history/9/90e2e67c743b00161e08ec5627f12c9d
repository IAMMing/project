package com.simple.authentication;

public class Param {
	/**
	 * 客户端加密串
	 */
	private String e;
	/**
	 * 客户端时间戳
	 */
	private Long t;
	/**
	 * 客户端验证策略
	 */
	private String p;
	/**
	 * 客户端请求次数
	 */
	private int n;

	public String getE() {
		return e;
	}

	public void setE(String e) {
		this.e = e;
	}

	public Long getT() {
		return t;
	}

	public void setT(Long t) {
		this.t = t;
	}

	public String getP() {
		return p;
	}

	public void setP(String p) {
		this.p = p;
	}

	public int getN() {
		return n;
	}

	public void setN(int n) {
		this.n = n;
	}

}
