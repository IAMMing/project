package cn.simple.domain;

import cn.simple.responsibility.impl.AbstractResponsibility;

/**
 * 发布流程
 * 
 * @author ldm
 * @Date 2016年6月21日
 */
public class PublishFlow {
	IValid validHouse;
	AbstractResponsibility ad;
	AbstractResponsibility pay;
	AbstractResponsibility save;
	public AbstractResponsibility getValidHouse() {
		return validHouse;
	}
	public void setValidHouse(AbstractResponsibility validHouse) {
		this.validHouse = validHouse;
	}
	public AbstractResponsibility getAd() {
		return ad;
	}
	public void setAd(AbstractResponsibility ad) {
		this.ad = ad;
	}
	public AbstractResponsibility getPay() {
		return pay;
	}
	public void setPay(AbstractResponsibility pay) {
		this.pay = pay;
	}
	public AbstractResponsibility getSave() {
		return save;
	}
	public void setSave(AbstractResponsibility save) {
		this.save = save;
	}
	
}
